{
  "expo": {
    "name": "reeple-app-ogogorchimadika",
    "slug": "reeple-app-ogogorchimadika",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/reeple.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6488EA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/reeple.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reepleapp.ogogorchimadika"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
